<view><view class="setting-body"><block wx:for="{{setting}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view hover-class="{{item.id!=0?'setting-click':'none'}}" id="{{'id_'+item.id}}" data-event-opts="{{[['tap',[['itemClick',['$event']]]]]}}" bindtap="__e"><view class="setting-line"><text class="setting-text">{{item.title}}</text><block wx:if="{{item.id==0}}"><switch class="setting-switch" name="recive" checked="{{item.onoff}}" data-event-opts="{{[['change',[['msgChange',['$event']]]]]}}" bindchange="__e"></switch></block><block wx:if="{{item.id==3}}"><text class="setting-cache">22M</text></block><block wx:if="{{item.id==2}}"><image class="arrows-right-setting" src="../../../static/icon/settings/arrows-right-setting.png"></image></block></view><view class="{{[(index!=setting.length-1)?'line':'']}}"></view></view></block></view></view>